import java.util.Scanner;

public class Rehearsal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);

		boolean a = true;
		double sum=0;
		int n = in.nextInt();
		for (int i = 1; i <= n; i++) {
			sum += (double)( 1 / i);
		}
		System.out.println(sum);
	}
}