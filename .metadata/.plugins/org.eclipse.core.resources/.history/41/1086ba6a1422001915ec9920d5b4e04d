import java.util.Scanner;

public class Rehearsal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int i;
		boolean a = false;
		OUT:

		for (i = 0; i < 123; i++) {

			System.out.println("恭喜你猜中了,数字是：" + "。");
			System.out.println("你猜了" + "次。");
			System.out.println();
			for (i = 0; i < 123; i++) {
				if (a) {
					System.out.println("恭喜你猜中了,数字是：" + "。");
					System.out.println("你猜了" + "次。");
					System.out.println();
					break OUT;
				}

			}
		}
	}
}